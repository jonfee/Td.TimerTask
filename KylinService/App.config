    <add key="SqlType" value="mssql" />
    <add key="Author" value="Jonfee" />
    <add key="Mobile" value="15920005942" />
    <add key="Email" value="337883612@qq.com" />
    <add key="QQ" value="337883612" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
